@inherits FeedbackComponent<string, bool>
<TextArea Value="@resourceVal" AutoSize="true" Style="height: 300px; overflow: scroll" />
<Divider />
<TextArea Value="@resourceFunctionVal" AutoSize="true" Style="height: 300px; overflow: scroll" />
@code {
    [Inject]
    public IResourceService resourceService { get; set; }
    [Inject]
    public IResourceFunctionService resourceFunctionService { get; set; }
    private string resourceVal = string.Empty;
    private string resourceFunctionVal = string.Empty;
    /// <summary>
    /// 页面初始化
    /// </summary>
    /// <returns></returns>
    protected override async Task OnInitializedAsync()
    {
        resourceVal = await resourceService.GetSeedData();
        resourceFunctionVal = await resourceFunctionService.GetSeedData();
    }
}
