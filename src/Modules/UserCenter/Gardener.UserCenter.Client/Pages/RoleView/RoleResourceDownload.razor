@inherits FeedbackComponent<string, bool>
<TextArea Value="@val" Style="max-height:80Vh;min-height:80Vh; overflow:scroll;"/>
@code {
    [Inject]
    public IRoleService roleService { get; set; }
    private string val = string.Empty;
    /// <summary>
    /// 页面初始化
    /// </summary>
    /// <returns></returns>
    protected override async Task OnInitializedAsync()
    {
        val = await roleService.GetRoleResourceSeedData();
    }
}
